<table id="example" class="table table-striped table-bordered" cellspacing="0" width="100%">
        <thead>
            <tr>
                <th> </th>
                <th>Nom</th>
                <th>Prénom</th>
                <th>Email</th>
                <th>Entreprise</th>
                <th>Date de naissance</th>
                <th>Civilité</th>
                <th>Ville - Pays</th>
                <th> Groupe</th>
            </tr>
        </thead>

        <tbody>
        {% set i = 1  %}
        {% for client in clients %}
            <tr>
                {% if client  %}
                <td>{{i}}</td>
                <td>{{client.nom}}</td>
                <td>{{client.prenom}}</td>
                <td>{{client.email}}</td>
                <td>{{client.entreprise}}</td>
                <td>{{client.dateNaissance|date("m/d/Y")}}</td>
                <td>{{client.civilite}}</td>
                <td>{{client.ville}}</td>
                <td>{{client.pays.countryName}}</td>
                {% endif %}
                {% set i = i+1 %}
            </tr>  
            {% endfor%}          
        </tbody>
    </table>


